rule "Evening On"
when
	Item Evening received command ON or
	Channel 'astro:sun:local:set#event' triggered START
	//Time cron "0 30 17 ? * SUN,MON,TUE,WED,THU,FRI,SAT *"
then
	logWarn("Bigboxer23", "Evening On")
	EveningOnOn.sendCommand(100)
	UpstairsBathroomLight.sendCommand(50)
	FrontPorchLights.sendCommand(45)
	UnderCabinets.sendCommand(50)
	MasterBathTub.sendCommand(25)
	Upstairs.sendCommand(45)
	Hallway.sendCommand(20)
	Sink.sendCommand(20)
	EntryWay.sendCommand(80)
	MainAccent.sendCommand(75)
end

rule "Evening Off"
when
	Item Evening received command OFF or
	Time cron "0 30 22 ? * SUN,MON,TUE,WED,THU *" or
	Time cron "0 30 23 ? * FRI,SAT *"
then
	logWarn("Bigboxer23", "Evening Off")
	EveningOffOff.sendCommand(0)
	EveningOffOff.sendCommand(OFF)
	EveningOffOn.sendCommand(25)
	EveningOffOn.sendCommand(ON)
	StairsAccent.sendCommand(75)
	HeatingSetpoint.sendCommand(68)
	CoolingSetpoint.sendCommand(75)
end