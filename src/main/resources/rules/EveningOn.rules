rule "Evening On Rule"
when
	Item EveningOn received command ON or
	Time cron "0 30 17 ? * SUN,MON,TUE,WED,THU,FRI,SAT *"
then
	EveningOnOn.sendCommand(100)
	UpstairsBathroomLight.sendCommand(50)
	FrontPorchLights.sendCommand(45)
	UnderCabinets.sendCommand(50)
	UpstairsBathroomTub.sendCommand(15)
	Upstairs.sendCommand(45)
	Hallway.sendCommand(20)
	Sink.sendCommand(20)
	MainAccent.sendCommand(75)
end