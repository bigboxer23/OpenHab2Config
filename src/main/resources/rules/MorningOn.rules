rule "Morning On Rule"
when
	Time cron "0 55 06 ? * MON,TUE,WED,THU,FRI *"
then
	MorningOn.sendCommand(100)
	MorningOn.sendCommand(ON)
	UnderCabinets.sendCommand(35)
	LivingRoomBack.sendCommand(65)
	EntryWay.sendCommand(80)
	MasterBathMain.sendCommand(50)
	Upstairs.sendCommand(50)
	StairsAccent.sendCommand(35)
end